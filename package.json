		"react-i18next": "^15.1.0",
		"web-vitals": "^2.1.4",
		"yup": "^1.6.1"
	},
	"scripts": {
		"generate-api-client": "swagger-typescript-api -p https://raw.githubusercontent.com/pagopa/rtp-apis/refs/heads/main/src/rtp/api/pagopa/send.openapi.yaml -o ./generated -n apiClient --axios",
		"generate-auth-client": "swagger-typescript-api -p https://raw.githubusercontent.com/pagopa/mil-auth/refs/heads/main/src/main/resources/META-INF/openapi.yaml -o ./generated/auth --axios --modular",
		"generate-data-contracts": "swagger-typescript-api  --modular --no-client -p https://raw.githubusercontent.com/pagopa/p4pa-pu-bff/refs/heads/develop/openapi/p4pa-pu-bff.openapi.yaml -o ./generated",
		"generate": "yarn generate-api-client && yarn generate-auth-client && yarn generate-data-contracts",
		"start": "yarn generate && vite",
		"build": "yarn generate && tsc -b && vite build",
		"preview": "vite preview",
		"lint": "eslint .",
		"test": "vitest"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@tanstack/router-devtools": "^1.98.3",
