		"@typescript-eslint/eslint-plugin": "^8.8.1",
		"@typescript-eslint/parser": "^8.8.1",
		"@vitejs/plugin-react": "^4.3.2",
		"dotenv": "^16.4.5",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react-refresh": "^0.4.14",
		"jsdom": "^25.0.1",
		"process": "^0.11.10",
		"rimraf": "^6.0.1",
